{
	"_version": "1.7.0",
	"sap.app": {
		"id": "POReportForSCM1",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		},
		"dataSources": {
			"GWSAMPLE_BASIC": {
				"uri": "/sap/opu/odata/iwbep/GWSAMPLE_BASIC/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "model/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_belize"]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "POReportForSCM1.view.V_Root",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.collaboration": {},
				"sap.m": {},
				"sap.ui.comp": {},
				"sap.ui.core": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "POReportForSCM1.i18n.i18n"
				}
			},
			"": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "GWSAMPLE_BASIC",
				"preload": true
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "POReportForSCM1.view",
				"controlId": "V_Main",
				"viewLevel": 1,
				"controlAggregation": "pages",
				"clearAggregation": true
			},
			"targets": {
				"Target_V_Input": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "V_Input",
					"viewLevel": 1
				},
				"Target_V_Count": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "V_Count",
					"viewLevel": 2
				}
			},
			"routes": [{
				"name": "Target_V_Input",
				"pattern": "",
				"titleTarget": "",
				"greedy": false,
				"target": ["Target_V_Input"]
			}, {
				"name": "Target_V_Count",
				"pattern": "V_Count/{p_DT}/{p_BP}/{p_PT}",
				"titleTarget": "",
				"greedy": false,
				"target": ["Target_V_Count"]
			}]
		}
	}
}